    <mass_balance>
        <ixx unit="SLUG*FT2"> 4216.3 </ixx>
        <iyy unit="SLUG*FT2"> 7608 </iyy>
        <izz unit="SLUG*FT2"> 10355 </izz>
        <ixz unit="SLUG*FT2"> -1000 </ixz>
        <emptywt unit="LBS"> 5000 </emptywt>
        <location name="CG" unit="IN">
            <x> 188.8 </x>
