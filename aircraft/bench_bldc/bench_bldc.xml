  <aerodynamics>
    <axis name="LIFT">
        </axis>
    <axis name="DRAG">
      <function name="aero/coefficient/CD0">
        <description>Overall Drag</description>
        <product>
          <property>aero/qbar-psf</property>
          <property>metrics/Sw-sqft</property>
          <value>1</value>
        </product>
      </function>
    </axis>
    <axis name="SIDE">
        </axis>
    <axis name="ROLL">
        </axis>
    <axis name="PITCH">
        </axis>
    <axis name="YAW">
        </axis>
  </aerodynamics>
  
  <output name="prop_perf.csv" type="CSV" rate="250">
  	<property>propulsion/engine/propeller-rpm</property>
  	<property>propulsion/engine[0]/thrust-lbs</property>
	<property>propulsion/engine[0]/prop_torque_ftlbs</property>
	<property>propulsion/engine/power-hp</property>

  </output>
