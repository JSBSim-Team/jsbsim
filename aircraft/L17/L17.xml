    XLOC 36
    YLOC 0
    ZLOC 0
    PITCH 0
    YAW 0
    FEED 0
    <AC_THRUSTER FILE="prop_75in2f">
      XLOC 36
      YLOC 0
      ZLOC 0
      PITCH 0
      YAW 0
    </AC_THRUSTER>
  </AC_ENGINE>
  <AC_TANK TYPE="FUEL" NUMBER="0">
    XLOC 158.4 
    YLOC 0 
    ZLOC -8.25 
    RADIUS 1 
    CAPACITY    20.0
    CONTENTS    10.0
  </AC_TANK>
  <AC_TANK TYPE="FUEL" NUMBER="1">
