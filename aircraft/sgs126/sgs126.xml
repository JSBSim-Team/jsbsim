        </contact>
        <contact type="BOGEY" name="RIGHT_WING">
            <location unit="IN">
                <x> 103.2 </x>
                <y> -20 </y>
                <z> -6.45 </z>
            </location>
            <static_friction> 0.8 </static_friction>
            <dynamic_friction> 0.5 </dynamic_friction>
