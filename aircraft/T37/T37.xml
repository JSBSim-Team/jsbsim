                    <pitch> 0.0 </pitch>
                    <yaw> 0.0 </yaw>
                </orient>
            </thruster>
        </engine>
        <engine file="J69-T25">
            <feed>1</feed>
            <thruster file="direct">
                <location unit="IN">
                    <x> 40 </x>
                    <y> 30 </y>
                    <z> 0 </z>
                </location>
                <orient unit="DEG">
                    <roll> 0.0 </roll>
                    <pitch> 0.0 </pitch>
