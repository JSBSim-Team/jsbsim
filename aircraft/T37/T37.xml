        <tank type="FUEL">    <!-- Tank number 0 -->
            <location unit="IN">
                <x> 0 </x>
                <y> 0 </y>
                <z> -8.7999 </z>
            </location>
            <capacity unit="LBS"> 700 </capacity>
            <contents unit="LBS"> 350 </contents>
        </tank>
        <tank type="FUEL">    <!-- Tank number 1 -->
            <location unit="IN">
                <x> 0 </x>
